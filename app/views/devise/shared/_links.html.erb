<%- if controller_name != 'sessions' %>
  <%= link_to t(:log_in_link), new_session_path(resource_name),{:class=>"pull-right need-help"} %><span class="clearfix"></span>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to t(:sign_up_link), new_registration_path(resource_name),{:class=>"pull-right need-help"} %><span class="clearfix"></span>
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to t(:password_forget_link), new_password_path(resource_name),{:class=>"pull-right need-help"} %><span class="clearfix"></span>
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to t(:not_confirmation_instructions_link), new_confirmation_path(resource_name),{:class=>"pull-right need-help"} %><span class="clearfix"></span>
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to t(:not_unlock_instructions_link), new_unlock_path(resource_name),{:class=>"pull-right need-help"} %><<span class="clearfix"></span>
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider),{:class=>"pull-right need-help"} %><span class="clearfix"></span>
  <% end -%>
<% end -%>
